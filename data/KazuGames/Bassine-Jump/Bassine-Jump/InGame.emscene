overlay InGame/TopLeft
{
	class PanelZone
	position 0% 0%
	size 40% 50%
	material HautGauche

	element InGame/TopLeft/Score
	{
		class MinigameText
		position 10% 10%
		size 15% 10%
		font_size 24.0
		text Score :
	}

	element InGame/TopLeft/ScoreTxt
	{
		class MinigameText
		position 30% 10%
		size 70% 10%
		font_size 24.0
		text Score : 0
	}
}

