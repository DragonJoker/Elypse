material title
{
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			depth_write off

			texture_unit
			{
				texture_alias 0
				texture title.png
				max_anisotropy 8
				filtering anisotropic anisotropic linear
			}
		}

	}

}

material wind
{
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			depth_write off

			texture_unit
			{
				texture_alias 0
				max_anisotropy 8
				filtering anisotropic anisotropic linear
				tex_border_colour 0.0 0.0 0.0 0.0
				tex_address_mode border
			}
		}
	}
}

