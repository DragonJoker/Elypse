
// OVERLAYS

overlay Title
{
	class PanelZone
	position -40% -22.5%
	size 80% 45%
	align center center
	material Titre
}

overlay Commandes
{
	class MinigameFrame
	position 25% 20%
	size 50% 60%
	border_size 32.0 32.0 32.0 32.0

	element Commandes/Retour
	{
		class PanelZone
		position 25% 78%
		size 50.00% 18.00%
		material Retour
		mouse_over_material RetourMO
	}

	element Commandes/Text
	{
		class MinigameText
		position 10% 22%
		size 80% 50%
		font_size 24.00
		text Maintenir le clic gauche afin d'orienter le lanceur, relacher le clic gauche pour lancer la bille. Fleches gauche et droite pour controler les flippers. Barre d'espace pour secouer le flipper, mais gare au Tilt
	}

	element Commandes/Title
	{
		class MinigameText
		position 0.00% 5.00%
		size 100% 10%
		font_size 40
		text Commandes
		text_align center
	}

}

