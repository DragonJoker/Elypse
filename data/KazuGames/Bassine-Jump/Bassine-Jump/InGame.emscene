overlay InGame/SubmitScore
{
	class PanelZone
	position 20% 70%
	size 60% 10%
	material Base/Invisible

	element InGame/SubmitScore/Text
	{
		class MinigameText
		position 0% 10%
		size 100% 80%
		font_size 60.0
		text Soumettre le score
		text_align center
	}
}

overlay Start
{
	class PanelZone
	align center center
	size 256 32
	position -128 -25%-16
	material Base/Invisible

	element Start/Display
	{
		class MinigameText
		size 100% 2%
		position 0% 48%
		font_size 100.0
		text_colour 1.0 1.0 1.0 1.0
		text 3
		text_align center
	}
}

overlay Title
{
	class PanelZone
	position -35% -55%
	size 70% 110%
	align center center
	material Titre
}

overlay InGame/Result
{
	class PanelZone
	position 25% 37.5%
	size 50% 25%
	material Gagne
}

overlay FinMiniGame
{
	class MinigameText
	position 0% 45%
	size 100% 10%
	font_size 40.0
	text_colour 0.0 0.0 1.0 1.0
	text 0
}