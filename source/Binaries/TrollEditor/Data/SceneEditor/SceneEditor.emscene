overlay SceneEditor/ObjectPositions
{
	class PanelZone
	position -180 -34
	size 360 24
	align bottom center
	material Base/LowAlphaBlack
	element SceneEditor/ObjectPositions/ObjectPosX/Panel
	{
		class PanelZone
		position 10 2
		size 100 20
		material Base/HighAlphaBrightGrey
		mouse_over_material Base/HighAlphaPureWhite
		
		element SceneEditor/ObjectPositions/ObjectPositionX
		{
			class TextZone
			position 5 5
			text 0
		}
	}
	element SceneEditor/ObjectPositions/ObjectPosY/Panel
