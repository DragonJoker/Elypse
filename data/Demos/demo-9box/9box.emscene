ambient_light 0.1 0.1 0.1 1

//AutoExport

//SceneNodes

scene_node CameraNode
{
	parent root node
	position 0 0 0
}


camera RootCamera
{
	position 0 0 120.0
	attach_to CameraNode
}

//Objects

scene_node 9box
{
	parent root node
	position 0 0 0
}

scene_node cable_alim
{
	parent 9box true
	position 100 0 0
}

scene_node cable_rj45
{
	parent 9box true
	position 100 0 0
}

scene_node info_dsl
{
	parent 9box true
	position 31.5 -2.3 20.3
}

