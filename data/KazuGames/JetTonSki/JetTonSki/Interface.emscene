overlay Jetski/Menu
{
	class PanelZone
	material BaseMenu
	position 0.00% 0.0%
	size 30.00% 100%

	element Jetski/Menu/Creer
	{
		class PanelZone
		position 3.00% 5%
		size 60.54% 10%
		material Creer
		mouse_over_material CreerMO
	}

	element Jetski/Menu/Rejoindre
	{
		class PanelZone
		position 3.00% 25%
		size 60.54% 10%
		material Rejoindre
		mouse_over_material RejoindreMO
	}

	element Jetski/Menu/Continuer
	{
		class PanelZone
		position 3.00% 15%
		size 60.54% 10%
		material Continuer
		mouse_over_material ContinuerMO
	}

	element Jetski/Menu/Recommencer
	{
		class PanelZone
		position 3.00% 15%
		size 60.54% 10%
		material Recommencer
		mouse_over_material RecommencerMO
	}

	element Jetski/Menu/Jouer
	{
		class PanelZone
		position 3.00% 15%
		size 60.54% 10%
		material Commencer
		mouse_over_material CommencerMO
	}

	element Jetski/Menu/Commandes
	{
		class PanelZone
		position 3.00% 45%
		size 60.54% 10%
		material Commandes
		mouse_over_material CommandesMO
	}

	element Jetski/Menu/Options
	{
		class PanelZone
		position 3.00% 85%
		size 60.54% 10%
		material Options
		mouse_over_material OptionsMO
	}

	element Jetski/Menu/Regles
	{
		class PanelZone
		position 3.00% 65%
		size 60.54% 10%
		material Regles
		mouse_over_material ReglesMO
	}
}

overlay Jetski/Commandes
{
	class MinigameFrame
	position -25% -35%
	size 50% 70%
	align center center
	border_size 32.0 32.0 32.0 32.0

	element Jetski/Commandes/Retour
	{
		class PanelZone
		position 25% 85%
		size 50.00% 15.00%
		material Retour
		mouse_over_material RetourMO
	}

	element Jetski/Commandes/Text
	{
		class MinigameText
		position 10% 20%
		size 80% 65%
		font_size 24.00
		text Les commandes utilisees sont les touches directionnelles du clavier.|Fleche haut pour accelerer, fleche bas pour freiner.|Fleche droite et gauche pour tourner, respectivement a droite et a gauche.
	}

	element Jetski/Commandes/Title
	{
		class MinigameText
		position 0.00% 5.00%
		size 100% 10%
		font_size 40
		text Commandes
		text_align center
	}
}

overlay Jetski/Regles
{
	class MinigameFrame
	position 25% 15%
	size 50% 70%
	border_size 32.0 32.0 32.0 32.0

	element Jetski/Regles/Retour
	{
		class PanelZone
		position 25% 85%
		size 50.00% 15.00%
		material Retour
		mouse_over_material RetourMO
	}

	element Jetski/Regles/Text1
	{
		class MinigameText
		position 10% 20%
		size 80% 65%
		font_size 24.00
		text Le conducteur du Jet-ski doit terminer le parcours le plus vite possible tout en ayant accumule le maximum d'anneaux d'or. Les anneaux d'or donnent des points. Les anneaux rouges accelerent l'engin durant 3 secondes.
	}

	element Jetski/Regles/Title
	{
		class MinigameText
		position 0.00% 5.00%
		size 100% 10%
		font_size 40.00
		text Regles
		text_align center
	}
}

overlay Jetski/Options
{
	class MinigameFrame
	position 25.00% 25%
	size 50.00% 50%
	border_size 32.0 32.0 32.0 32.0

	element Jetski/Options/Title
	{
		class MinigameText
		position 40.00% 5%
		size 70% 25%
		font_size 40.0
		text Options
	}

	element Jetski/Options/Music
	{
		class MinigameText
		position 20.00% 25.00%
		size 0.12% 1.14%
		font_size 24.00
		text Musique
	}

	element Jetski/Options/MusicOnOff
	{
		class PanelZone
		position 20.00% 40.00%
		size 25% 25%
		material On
	}

	element Jetski/Options/RegletteMusic
	{
		class PanelZone
		position 25.00% 35.00%
		size 60% 15%
		material Reglette

		element Jetski/Options/Music/TicContainer
		{
			class PanelZone
			position 7% 0%
			size 85% 100%
			material Base/Invisible

			element Jetski/Options/TicMusic
			{
				class PanelZone
				position 80.00% 0.00%
				size 20.00% 100.00%
				material Tic
			}
		}
	}

	element Jetski/Options/Sound
	{
		class MinigameText
		position 20.00% 50%
		size 0.12% 1.14%
		font_size 24.00
		text Sons
	}

	element Jetski/Options/SoundOnOff
	{
		class PanelZone
		position 20.00% 65%
		size 25% 25%
		material On
	}

	element Jetski/Options/RegletteSound
	{
		class PanelZone
		position 25.00% 60%
		size 60% 15%
		material Reglette

		element Jetski/Options/Sound/TicContainer
		{
			class PanelZone
			position 7% 0%
			size 85% 100%
			material Base/Invisible

			element Jetski/Options/TicSound
			{
				class PanelZone
				position 80.00% 0.00%
				size 20.00% 100.00%
				material Tic
			}
		}
	}

	element Jetski/Options/Retour
	{
		class PanelZone
		position 25.00% 75.00%
		size 50.00% 20.00%
		material Retour
		mouse_over_material RetourMO
	}
}

