overlay Title
{
	class PanelZone
	position -20% -22.5%
	size 60% 45%
	align center center
	material InterfaceLimbo/Title
}

overlay Commandes
{
	class MinigameFrame
	position -25% -25%
	size 50% 50%
	align center center
	border_size 32.0 32.0 32.0 32.0

	element Commandes/Retour
	{
		class PanelZone
		position 25% 75%
		size 50.00% 20.00%
		material Retour
		mouse_over_material RetourMO
	}

	element Commandes/Text
	{
		class MinigameText
		position 10% 25%
		size 80% 50%
		font_size 24.00
		text Appuyez sur la bonne touche au moment ou une fleche touche un crane
	}

	element Commandes/Title
	{
		class MinigameText
		position 0.00% 5.00%
		size 100% 10%
		font_size 40
		text Commandes
		text_align center
	}
}

