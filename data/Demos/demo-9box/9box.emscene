{
	parent 9box true
	position 31.5 -2.3 -4.4
}

scene_node info_pwr
{
	parent 9box true
	position 31.5 -2.3 -20.6
}

scene_node info_led
{
	parent 9box true
	position 0.0 6.2 -21.6
}

scene_node info_reset
{
	parent 9box true
	position -31.4 -3.3 -18.5
}


object 9box
{
	mesh 9box.mesh
	attach_to 9box
}

