overlay SceneEditor/Saved
{
	class BorderPanelZone
	position -120 -60
	size 110 50
	align bottom right
	material Base/MediumAlphaBlack
	border_material Base/HighAlphaPureWhite
	border_size 2 2 2 2
	element SceneEditor/Saved/Text
	{
		class TextZone
		text Saved
		text_align both
	}
}

