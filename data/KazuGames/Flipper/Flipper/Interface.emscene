overlay Options
{
	class MinigameFrame
	position 25.00% 25%
	size 50.00% 50%
	border_size 32.0 32.0 32.0 32.0

	element Options/Title
	{
		class MinigameText
		position 40.00% 5%
		size 70% 25%
		font_size 40.0
		text Options
	}

	element Options/Music
	{
		class MinigameText
		position 20.00% 25.00%
		size 0.12% 1.14%
		font_size 24.00
		text Musique
	}

	element Options/MusicOnOff
	{
		class PanelZone
		position 20.00% 39.00%
		size 6% 8%
		material On
	}

	element Options/RegletteMusic
	{
		class PanelZone
		position 25.00% 35.00%
		size 60% 15%
		material Reglette

		element Options/Music/TicContainer
		{
			class PanelZone
			position 7% 0%
			size 85% 100%
			material Base/Invisible

			element Options/TicMusic
			{
				class PanelZone
				position 80.00% 0.00%
				size 20.00% 100.00%
				material Tic
			}
		}
	}

	element Options/Sound
	{
		class MinigameText
		position 20.00% 50%
		size 0.12% 1.14%
		font_size 24.00
		text Sons
	}

	element Options/SoundOnOff
	{
		class PanelZone
		position 20.00% 64%
		size 6% 8%
		material On
	}

	element Options/RegletteSound
	{
		class PanelZone
		position 25.00% 60%
		size 60% 15%
		material Reglette

		element Options/Sound/TicContainer
		{
			class PanelZone
			position 7% 0%
			size 85% 100%
			material Base/Invisible

			element Options/TicSound
			{
				class PanelZone
				position 80.00% 0.00%
				size 20.00% 100.00%
				material Tic
			}
		}
	}

	element Options/Retour
	{
		class PanelZone
		position 25.00% 75.00%
		size 50.00% 20.00%
		material Retour
		mouse_over_material RetourMO
	}
}

overlay Fin
{
	class PanelZone
	position 10% 30%
	size 80% 40%
	material Base/Invisible

	element Fin/Fin
	{
		class PanelZone
		position 25% 0%
		size 50% 60%
		material Perdu
	}
	element Fin/Score
	{
		class PanelZone
		material Base/Invisible
		position 20% 50%
		size 60% 20%
		element Fin/Score/Titre
		{
			class MinigameText
			position 0% 0%
			size 50% 100%
			font_size 100.0
			text_colour 1.0 0.0 0.0 1.0
			text Score :
		}
		element Fin/Score/Text
		{
			class MinigameText
			position 50% 0%
			size 100% 100%
			font_size 100.0
			text_colour 1.0 0.0 0.0 1.0
			text 0
			text_align center
		}
	}
}

