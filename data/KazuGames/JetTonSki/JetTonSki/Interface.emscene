overlay Jetski/AvailableGames
{
	class MinigameFrame
	position 25% 0%
	size 50% 100%
	border_size 32.0 32.0 32.0 32.0

	element Jetski/AvailableGames/Title
	{
		class MinigameText
		position 0% 2%
		size 100% 10%
		font_size 32.0
		text Choix de partie
		text_align center
	}

	element Jetski/AvailableGames/ButtonJoin
	{
		class PanelZone
		position 15% 90%
		size 30% 10%
		material Rejoindre
		mouse_over_material RejoindreMO
	}

	element Jetski/AvailableGames/Retour
	{
		class PanelZone
		position 55% 90%
		size 30% 10%
		material Retour
		mouse_over_material RetourMO
	}

	element Jetski/AvailableGames/Separators
	{
		class NoBorderMenu
		position 10% 10%
		size 80% 80%
		border_size 32.0 32.0 32.0 32.0
	
		element Jetski/AvailableGames/TitleName
		{
			class PanelZone
			position 0% 0%
			size 60% 7%
			material Base/Invisible

			element Jetski/AvailableGames/TitleName/Text
			{
				class MinigameText
				font_size 24.0
				position 50% 30%
				text Createurs
				text_align center
			}
		}

		element Jetski/AvailableGames/TitleNbPlayers
		{
			class PanelZone
			position 63% 0%
			size 37% 7%
			material Base/Invisible

			element Jetski/AvailableGames/TitleNbPlayers/Text
			{
				class MinigameText
				font_size 24.0
				position 50% 30%
				text Nb. Joueurs
				text_align center
			}
		}

		element Jetski/AvailableGames/VerticUp
		{
			class PanelZone
			position 60% 0%
			size 4% 9%
			material VerticSeparator
		}

		element Jetski/AvailableGames/VerticDown
		{
			class PanelZone
			position 60% 10%
			size 4% 90%
			material VerticSeparator
		}

		element Jetski/AvailableGames/Horizontal
		{
			class PanelZone
			position 0% 8%
			size 99.9% 3%
			material HorizSeparator
		}

		element Jetski/AvailableGames/List
		{
			class PanelZone
			position 5% 12%
			size 90% 88%
			material Base/Invisible
		}

		element Jetski/AvailableGames/Started
		{
			class MinigameText
			position 15% 95%
			size 90% 8%
			font_size 22.0
			text Parties en cours
		}

		element Jetski/AvailableGames/StartedNb
		{
			class MinigameText
			position 65% 95%
			size 90% 8%
			font_size 22.0
		}
	}
}

