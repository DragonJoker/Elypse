// OVERLAYS

overlay Custo/MessageBox
{
	class ChatPanel
	position 35% 70%
	size 30% 20%
	border_size 32.0 32.0 32.0 32.0
	z_index 499

	element Custo/MessageBox/Title
	{
		class PanelZone
		position 10% 10%
		size 80% 20%
		material Base/Invisible

		element Custo/MessageBox/Title/Text
		{
			class ChatText
			font_size 30
			position 0% 10%
			size 100% 80%
			text TITLE
			text_align center
		}
	}

	element Custo/MessageBox/Caption
	{
		class PanelZone
		position 10% 30%
		size 80% 40%
		material Base/Invisible

		element Custo/MessageBox/Caption/Text
		{
			class ChatText
			font_size 20
			position 0% 10%
			size 100% 80%
			text CAPTION
			text_align center
		}
	}

	element Custo/MessageBox/ButtonOK
	{
		class BorderPanelZone
		position 25% 70%
		size 50% 25%
		material Chat/Validate
		mouse_over_material Chat/ValidateMO
	}
}

