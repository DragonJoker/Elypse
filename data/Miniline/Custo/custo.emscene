// SCENE_NODES

scene_node PersoNode
{
	position 100.0 0.0 0.0
}

scene_node DecorNode
{
	position 100.0 190.0 0.0
}

scene_node CameraNode
{
	parent PersoNode
	position 0.0 250.0 0.0
}

// OBJECTS

object Fond
{
	mesh local.mesh
	attach_to DecorNode
}

// LIGHTS

light light_01
{
	type directional
	direction 0.0 -1.0 -1.0
	diffuse 0.90 0.90 0.90 1.00
	specular 0.70 0.70 0.70 1.00
}


// CAMERA

camera RootCamera
{
	attach_to CameraNode
	position 0.0 0.0 600.0
}